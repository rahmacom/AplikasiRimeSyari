<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.profil_kontak.ProfilKontakFragment">

    <TextView
        android:id="@+id/tv_profil_kontak_ig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@rime.syari"
        app:layout_constraintBottom_toBottomOf="@+id/iv_profil_kontak_ig"
        app:layout_constraintStart_toEndOf="@+id/iv_profil_kontak_ig"
        app:layout_constraintTop_toTopOf="@+id/iv_profil_kontak_ig" />

    <TextView
        android:id="@+id/tv_profil_kontak_wa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="082281820631"
        app:layout_constraintBottom_toBottomOf="@+id/iv_profil_kontak_wa"
        app:layout_constraintStart_toEndOf="@+id/iv_profil_kontak_wa"
        app:layout_constraintTop_toTopOf="@+id/iv_profil_kontak_wa" />

    <ImageView
        android:id="@+id/iv_profil_kontak_wa"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="320dp"
        android:src="@drawable/icon_brand_wa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_profil_kontak_ig"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/icon_brand_ig"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_profil_kontak_wa" />

</androidx.constraintlayout.widget.ConstraintLayout>